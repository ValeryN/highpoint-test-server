<% @require ojster.Template %>

<% @template dev.developer.ui.linkItem.templates.Base %>
<% @inherits ojster.Template %>


<% @block main { %>
<div class="<% @css %>">
  <% @call link %>
  <% if (d.inputEnabled) { %>
    :&nbsp;<%-d.inputPrefix%><% @call input %><%-d.inputSuffix%>
  <% } %>
</div>
<% @block main } %>


<% @block link { %>
<span class="<% @css +link %>"><%-d.linkContent%></span>
<% @block link } %>


<% @block input { %>
<input class="<% @css +input %>" value="<%=d.defaultInputValue%>"/>
<% @block input } %>
